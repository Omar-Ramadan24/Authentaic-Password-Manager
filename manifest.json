{
  "manifest_version": 3,
  "name": "Simple Password Manager",
  "description": "A simple Chrome extension for managing passwords.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  }
}
